<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.asu.cse545</groupId>
  <artifactId>sbs</artifactId>
  <packaging>war</packaging>
  <version>0.0.1-SNAPSHOT</version>
  <name>Secure Bank System</name>


  <properties>
    <spring.version>4.2.1.RELEASE</spring.version>
    <hibernate.version>5.0.1.Final</hibernate.version>
    <log4j.version>1.2.17</log4j.version>
    <jdk.version>1.7</jdk.version>
    <context.path>SecureBankSystem</context.path>
    <spring.security.version>4.0.2.RELEASE</spring.security.version>
  </properties>
  <developers>
    <developer>
      <id>Nagarjuna myla</id>
      <email>nmyla@asu.edu</email>
      <organization>asu.edu</organization>
      <organizationUrl>http://www.asu.edu</organizationUrl>
      <roles>
        <role>Student</role>
      </roles>
      <timezone>-7:00</timezone>
    </developer>
  </developers>

  <build>
    <finalName>${project.artifactId}</finalName>

    <plugins>
      <!-- Maven compiler plugin -->
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>2.3.2</version>
        <configuration>
          <source>${jdk.version}</source>
          <target>${jdk.version}</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
